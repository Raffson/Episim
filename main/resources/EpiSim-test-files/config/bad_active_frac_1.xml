<?xml version="1.0" encoding="utf-8"?>
<popgen>

    <data_files>
        <cities>flanders_cities_test.csv</cities>
        <commuting>flanders_commuting_test.csv</commuting>
        <households>households_flanders_test.xml</households>
    </data_files>

    <rng>
        <seed>0</seed>
        <type>lcg64</type>
    </rng>

    <pop_info>
        <pop_total>4341923</pop_total>


        <!-- fraction of people able to work that is student (18y-26y) -->
        <fraction_students>0.5</fraction_students>
        <!-- commuting fraction of students -->
        <fraction_commuting_students>0.5</fraction_commuting_students>

        <!-- fraction of people that is able to work and actually working -->
        <fraction_active_workers>-1.1</fraction_active_workers>
        <!-- commuting fraction of active workers -->
        <fraction_commuting_workers>0.5</fraction_commuting_workers>
    </pop_info>

    <contactpool_info>
	<average_size>20</average_size>
        <school>
            <size>500</size>
        </school>
        <college>
            <size>3000</size>
            <cities>10</cities>
        </college>
        <community>
            <size>2000</size>
        </community>
        <workplace>
            <size>20</size>
        </workplace>
    </contactpool_info>

</popgen>
